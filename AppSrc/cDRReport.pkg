    { Description += "The NamedValues is filled in the ExportReport.wo or ExportReportDialog.wo and depends" }
    { Description += "on the export format. Each export format has different named values" }
    { MethodType = Method }
    Procedure ExportReportToDownloadURL tNameValuePair[] NamedValues
        String sReportId sUrl sExportFormat sFileNameWithoutPath
