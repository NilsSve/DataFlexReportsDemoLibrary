    { Description = "Helper function to construct a folder path ending on Cache and check the existence" }
    { Visibility = Private }
    Function CheckReportsCacheFolder String sReportsFolder String sDirectorySeparator String sDefaultCacheFolderName Returns String
        String sCacheFolder
        Boolean bIsDirectory

        // Make sure path ends with a directory separator before the default name for the cache folder will be added.
        If (Right (sReportsFolder, 1) <> sDirectorySeparator) Begin
            Move (sReportsFolder - sDirectorySeparator) to sReportsFolder
        End
        Move (sReportsFolder - sDefaultCacheFolderName - sDirectorySeparator) to sCacheFolder
        Move (IsDirectory (sCacheFolder)) to bIsDirectory
        If (bIsDirectory) Begin
            Function_Return sCacheFolder
        End

        Function_Return ""
    End_Function

