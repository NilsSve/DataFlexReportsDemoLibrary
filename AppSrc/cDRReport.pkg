        Boolean bAllPages
        DRPDFExportOptions PDFExportOptions
        DRImageExportOptions ImageExportOptions
        drXLSExportOptions XLSExportOptions
        drRTFExportOptions RTFExportOptions
        drCSVExportOptions CSVExportOptions
